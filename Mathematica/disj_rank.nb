Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"helper", "[", 
    RowBox[{"u_", ",", "v_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "s", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Quiet", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"_", ",", "s", ",", "_"}], "}"}], "=", 
        RowBox[{"SingularValueDecomposition", "[", 
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Conjugate", "[", "u", "]"}], ".", "v"}], "}"}], "}"}], 
         "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Sin", "[", 
       RowBox[{"ArcCos", "[", 
        RowBox[{"s", "[", 
