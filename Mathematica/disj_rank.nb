Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"eigvecDist", "[", 
    RowBox[{"m_", ",", "t_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "u", ",", "v", ",", "eig", ",", "eigvec", ",", "i1", ",", "i2"}], "}"}],
      ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"eig", ",", "eigvec"}], "}"}], "=", 
       RowBox[{"Eigensystem", "[", 
        RowBox[{"N", "[", 
         RowBox[{"Transpose", "[", "m", "]"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"i1", "=", 
       RowBox[{
        RowBox[{"Ordering", "[", 
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"eig", "-", "t"}], "]"}], ",", "1"}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"i2", "=", 
       RowBox[{
        RowBox[{"Ordering", "[", 
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"eig", "-", 
            RowBox[{"eig", "[", 
             RowBox[{"[", "i1", "]"}], "]"}]}], "]"}], ",", "2"}], "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"u", "=", 
       RowBox[{"eigvec", "[", 
        RowBox[{"[", "i1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"v", "=", 
       RowBox[{"eigvec", "[", 
        RowBox[{"[", "i2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"helper", "[", 
       RowBox[{"u", ",", "v"}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"eigvalDist", "[", 
    RowBox[{"m_", ",", "t_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"eig", ",", "i1", ",", "i2"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
