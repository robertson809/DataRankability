Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"condorcetM", "[", "l_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "diag", ",", "eigval", ",", "d1", ",", "d2"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "l", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"diag", "=", 
       RowBox[{"Diagonal", "[", "l", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"eigval", "=", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"Eigenvalues", "[", "l", "]"}], ",", "16"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"d1", "=", 
       RowBox[{"Min", "[", 
        RowBox[{"Abs", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"n", "-", "1"}], ")"}], "-", "diag"}], "]"}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"d2", "=", 
       RowBox[{"Min", "[", 
        RowBox[{"Abs", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"n", "-", "1"}], ")"}], "-", "eigval"}], "]"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"1", "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"d1", "+", "d2"}], ")"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "*", 
          RowBox[{"(", 
           RowBox[{"n", "-", "1"}], ")"}]}], ")"}]}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
